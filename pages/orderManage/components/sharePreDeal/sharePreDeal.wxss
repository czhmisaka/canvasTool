/* pages/orderManage/components/sharePreDeal/sharePreDeal.wxss */
.wholeScreen {
    position: fixed;
    top: 0rpx;
    left: 0rpx;
    width: 100vw;
    height: 100vh;
    background-color: white;
    z-index: 100000;
}


.logoImage {
    margin: 262rpx 215rpx;
    margin-bottom: 80rpx;
    width: 320rpx;
    height: 320rpx;
}

.buttonBox {
    margin: 40rpx 95rpx;
    width: 560rpx;
    height: 92rpx;
    border-radius: 46rpx;
    border: 1px rgba(52,62,223,1) solid;
    display: flex;
    justify-content: center;
    line-height: 92rpx;
    background-color: rgba(0, 0, 0, 0);
}

.buttonBox>.icon {
    margin: 4rpx 10rpx;
}

.logoImage>image{
    width: 100%;
    height: 100%;
}
.icon>image {
    width: 100%;
    height: 100%;
}
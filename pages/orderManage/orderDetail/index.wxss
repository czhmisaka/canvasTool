page{
  background:#F7F8FA;
  height:100%;
  position: relative;
}
.addressAdmin{
  height:160rpx;
  background: #ffffff;
  border-radius:8px;
  position: relative;
}
.addressName{
  display: inline-block;
  width:100rpx;
  height:45rpx;
  font-size:32rpx;
  color:#3D3D3D;
  overflow: hidden;
  margin-top:31rpx;
  margin-left:44rpx;
  line-height: 45rpx;
}
.addressPhone{
  display: inline-block;
  width:190rpx;
  height:45rpx;
  font-size:32rpx;
  color:#999999;
  overflow: hidden;
  margin-left:12rpx;
  line-height: 45rpx;
}
.addressDetail{
  display: inline-block;
  height:40rpx;
  font-size:32rpx;
  color:#3D3D3D;
  overflow: hidden;
  margin-left:44rpx;
  line-height: 40rpx;
  width:500rpx;
}
.amend{
  color:#999999;
  font-size:28rpx;
  width:56rpx;
  height:40rpx;
  line-height: 40rpx;
  text-align: center;
  position: absolute;
  top:60rpx;
  left:600rpx;
}
.amendcon{
  color:#cccccc;
  position: absolute;
  top:48rpx;
  left:668rpx;
  font-size:44rpx;
}
/* 收货地址 */
.shopAddressBox{ 
  height:140rpx;
  margin:15rpx;
  background: #ffffff;
  border-radius: 10rpx;
  overflow: hidden;
  position: relative;
}
.shopAddress{
  width:500rpx;
  margin-left:30rpx;
  margin-top:20rpx;
  overflow: hidden;
  display: inline-block;
}
.shopAddress .recipients{
  height:50rpx;
  line-height: 50rpx;
  color:#323232;
  font-size:33rpx;
}
.shopAddress .profile{
  height:50rpx;
  line-height: 50rpx;
  color:#323232;
  font-size:33rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.shopAddressBox .amend{
  width:100rpx;
  height:50rpx;
  font-size:30rpx;
  color:#ee5545;
  display: inline-block;
  margin-top:-50rpx;
  position: absolute;
  left:600rpx;
  top:98rpx;
}
/* 商品详情 */
.orderDetailBox{
  /* height:700rpx; */
  background:#ffffff;
  margin-top:20rpx;
}
.orderDetailTitle{
  width:208rpx;
  overflow: hidden;
  height:45rpx;
  margin-left:34rpx;
  line-height: 45rpx;
  display: inline-block;
  color:#3D3D3D;
  font-weight:500;
  font-size:32rpx;
  margin-top:24rpx;
}
.orderState{
  height:93rpx;
  margin-left:34rpx;
  line-height: 93rpx;
  font-weight:500;
  font-size:24rpx;
  float:right;
  margin-right:34rpx;
  text-align: right;
  color:#FD3C31;
}
.orderDetailBox .orderItemBox{
  height:100%;
  border-top:1rpx solid #E6E6E6;
  margin-top:10rpx;
}
.orderDetailImg{
  width:164rpx;
  height:164rpx;
  margin-left:34rpx;
  margin-top:32rpx;
  float:left;
}
.orderDetailName{
  float:left;
  height:80rpx;
  width:494rpx;
  margin-top:32rpx;
  margin-left:24rpx;
  color:#3D3D3D;
  font-size:28rpx;
  display: -webkit-box;    
  -webkit-box-orient: vertical;    
  -webkit-line-clamp: 2;    
  overflow: hidden;
}
.orderDetailPrice{
  float:left;
  height:40rpx;
  width:450rpx;
  margin-top:40rpx;
  margin-left:24rpx;
  color:#EE4866;
  margin-bottom:50rpx;
  font-size:28rpx;
  overflow: hidden;
}
.orderDetailSizeBox{
  width:680rpx;
  height:33rpx;
  margin-left:34rpx;
  line-height: 33rpx;
  display: flex;
  justify-content: space-between;  
  color:#3D3D3D; 
  font-size:24rpx;
  margin-bottom:16rpx;
}
.storeMoneyBox{
  display: flex;
  justify-content: space-between;  
  width:680rpx;
  height:33rpx;
  margin-left:34rpx;
  margin-top:10rpx;
  line-height: 33rpx;
  color:#3D3D3D;
  font-size:24rpx;
  padding-bottom:24rpx;
}
/* 订单总价 */
.orderTotalPriceBox{
  display: flex;
  justify-content: space-between;  
  width:100%;
  height:70rpx;
  border-top:1rpx solid #E6E6E6;
  line-height: 70rpx;
  color:#3D3D3D; 
  font-size: 28rpx;
}
.orderTotalPrice{
  padding-left:30rpx;
  width:120rpx;
  height:40rpx;
  font-size:28rpx;
  color:#3D3D3D;
  line-height:40rpx;
  overflow: hidden;
  margin-top:15rpx;
  display: inline-block;
}
.orderPrice{
  width:200rpx;
  height:45rpx;
  font-size:32rpx;
  color:#EE4866;
  line-height:45rpx;
  overflow: hidden;
  margin-top:15rpx;
  display: inline-block;
  padding-right:34rpx;
  text-align: right;
}
/* 订单状态 */
.orderStatusBox{
  height:200rpx;
  color:#3D3D3D;
  overflow: hidden;
  background: #ffffff;
  padding-top:24rpx;
  padding-left:34rpx;
  margin-top:20rpx;
  margin-bottom:20rpx;
}
.orderNumber{
  height:40rpx;
  line-height: 40rpx;
}
.placeOrderTime{
  height:40rpx;
  line-height: 40rpx;
  margin:16rpx 0 ;
}
.takeOrderTime{
  height:40rpx;
  line-height: 40rpx;
}
.orderTabbar{
  height:120rpx;
  width:100%;
  background: #ffffff;
  /* display: flex; */
}
.cancel{
  width:160rpx;
  height:72rpx;
  border-radius:46rpx;
  border:1rpx solid rgba(61,61,61,1);
  text-align: center;
  line-height: 72rpx;
  font-size:32rpx;
  font-weight:400;
  color:rgba(61,61,61,1);
  float: right;
  margin-top:24rpx;
  margin-left:70rpx;
}

page {
  background: #F7F8FA;
  height: 100%;
  position: relative;
}

.addressAdmin {
  width: 694rpx;
  height: auto;
  display: inline-block;
  background: #ffffff;
  border-radius: 8px;
  position: relative;
  padding: 8rpx;
  margin: 10rpx 20rpx;
  padding-bottom: 40rpx;
  margin-top: 20rpx;
}

.gray {
  color: #999999;
}

.small {
  font-size: 20rpx;
}

.addressAdmin>.box {
  width: calc(100% - 48rpx);
  height: auto;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(61, 61, 61, 1);
  line-height: 60rpx;
  /* margin-top: 32rpx; */
  margin-left: 24rpx;
}

.box>.bar {
  float: left;
  width: 4rpx;
  border-radius: 2rpx;
  height: 28rpx;
  margin: 16rpx 24rpx;
}

.box>.red {
  background-color: red;
}

.box>.black {
  background-color: black;
}

.amend {
  color: #999999;
  font-size: 28rpx;
  width: 56rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  position: absolute;
  top: 60rpx;
  left: 600rpx;
}

.amendcon {
  color: #cccccc;
  position: absolute;
  top: 48rpx;
  left: 668rpx;
  font-size: 44rpx;
}

/* 收货地址 */
.shopAddressBox {
  height: 140rpx;
  margin: 15rpx;
  background: #ffffff;
  border-radius: 10rpx;
  overflow: hidden;
  position: relative;
}

.shopAddress {
  width: 500rpx;
  margin-left: 30rpx;
  margin-top: 20rpx;
  overflow: hidden;
  display: inline-block;
}

.shopAddress .recipients {
  height: 50rpx;
  line-height: 50rpx;
  color: #323232;
  font-size: 33rpx;
}

.shopAddress .profile {
  height: 50rpx;
  line-height: 50rpx;
  color: #323232;
  font-size: 33rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.shopAddressBox .amend {
  width: 100rpx;
  height: 50rpx;
  font-size: 30rpx;
  color: #ee5545;
  display: inline-block;
  margin-top: -50rpx;
  position: absolute;
  left: 600rpx;
  top: 98rpx;
}

/* 商品详情 */
.orderDetailBox {
  /* height:700rpx; */
  background: #ffffff;
  border-radius: 16rpx;
  margin: 10rpx 20rpx;
  width: calc(100% - 40rpx);
}

.orderDetailTitle {
  width: calc(100% - 160rpx);
  overflow: hidden;
  height: 45rpx;
  margin-left: 34rpx;
  line-height: 45rpx;
  display: inline-block;
  color: #3D3D3D;
  font-weight: 500;
  font-size: 32rpx;
  margin-top: 24rpx;
}

.orderState {
  height: 93rpx;
  margin-left: 34rpx;
  line-height: 93rpx;
  font-weight: 500;
  font-size: 24rpx;
  float: right;
  margin-right: 34rpx;
  text-align: right;
  color: #FD3C31;
}

.orderDetailBox .orderItemBox {
  height: 100%;
  border-top: 1rpx solid #E6E6E6;
  margin-top: 10rpx;
}

.orderDetailImg {
  width: 164rpx;
  height: 164rpx;
  margin-left: 24rpx;
  margin-top: 24rpx;
  float: left;
  border-radius: 16rpx;
}

.orderDetailName {
  float: left;
  height: 80rpx;
  width: 479rpx;
  margin-top: 32rpx;
  margin-left: 24rpx;
  color: #3D3D3D;
  font-size: 28rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}

.orderDetailPrice {
  float: right;
  text-align: right;
  height: 40rpx;
  width: 450rpx;
  margin-top: 40rpx;
  margin-right: 24rpx;
  /* color: #EE4866; */
  margin-bottom: 24rpx;
  font-size: 28rpx;
  overflow: hidden;
}

.orderDetailPrice>.gray {
  color: #999;
}

.orderDetailSizeBox {
  width: calc(710rpx - 48rpx);
  height: 33rpx;
  margin-left: 24rpx;
  line-height: 33rpx;
  display: flex;
  justify-content: space-between;
  color: #3D3D3D;
  font-size: 24rpx;
  margin-bottom: 16rpx;
}

.storeMoneyBox {
  display: flex;
  justify-content: space-between;
  width: calc(710rpx - 48rpx);
  height: 33rpx;
  margin-left: 24rpx;
  margin-top: 10rpx;
  line-height: 33rpx;
  color: #3D3D3D;
  font-size: 24rpx;
  padding-bottom: 24rpx;
}

/* 订单总价 */
.orderTotalPriceBox {
  display: flex;
  justify-content: space-between;
  width: 100%;
  height: 70rpx;
  border-top: 1rpx solid #E6E6E6;
  line-height: 70rpx;
  color: #3D3D3D;
  font-size: 28rpx;
}

.orderTotalPrice {
  padding-left: 30rpx;
  width: 120rpx;
  height: 40rpx;
  font-size: 28rpx;
  color: #3D3D3D;
  line-height: 40rpx;
  overflow: hidden;
  margin-top: 15rpx;
  display: inline-block;
}

.orderPrice {
  width: 200rpx;
  height: 45rpx;
  font-size: 32rpx;
  color: #EE4866;
  line-height: 45rpx;
  overflow: hidden;
  margin-top: 15rpx;
  display: inline-block;
  padding-right: 34rpx;
  text-align: right;
}

/* 订单状态 */
.orderStatusBox {
  height: auto;
  display: inline-block;
  color: #3D3D3D;
  overflow: hidden;
  background: #ffffff;
  margin: 10rpx 20rpx;
  border-radius: 16rpx;
  width: 710rpx;
  margin-top: 5rpx;
  padding: 8rpx 0rpx;
  margin-bottom: 160rpx;
}

.orderStatusBox>.box {
  width: calc(100% - 48rpx);
  height: 28rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(61, 61, 61, 1);
  line-height: 28rpx;
  margin: 24rpx;
}

.orderTabbar {
  height: 120rpx;
  width: 100%;
  background: #ffffff;
  display: flex;
  justify-content: center;
  position: fixed;
  bottom: 0rpx;
}

.cancel {
  width: 160rpx;
  height: 72rpx;
  border-radius: 46rpx;
  border: 1rpx solid rgba(61, 61, 61, 1);
  text-align: center;
  line-height: 72rpx;
  font-size: 32rpx;
  font-weight: 400;
  color: rgba(61, 61, 61, 1);
  float: right;
  margin-top: 24rpx;
  margin-left: 70rpx;
}

.transmit {
  float: right;
  width: 240rpx;
  height: 72rpx;
  border-radius: 46rpx;
  text-align: center;
  line-height: 72rpx;
  font-size: 32rpx;
  font-weight: 400;
  margin-top: 24rpx;
  margin-right: 34rpx;
  margin-left: 20rpx;
  border: 1rpx rgba(61, 61, 61, 1) solid;
}

.highLight {
  color: #FFFFFF;
  background: rgba(61, 61, 61, 1);
}

.wholeScreen {
  z-index: 1000;
  position: fixed;
  width: 100vw;
  height: 100vh;
  /* background-color: rgba(0,0,0,0.1); */
}

.wholeScreen>.bg {
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.1);
  z-index: 1000;
  position: absolute;
}

.wholeScreen>.msgBox {
  z-index: 1001;
  width: 600rpx;
  height: auto;
  display: inline-block;
}

.msgBox>.title {
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  border-bottom: 1rpx rgba(0, 0, 0, 0.2) solid;
  text-align: center;
}

.msgBox>.msg {
  padding: 40rpx;
  display: inline-block;
  margin: 10rpx;
}

.section {
  width: 710rpx;
  margin: 10rpx 20rpx;
  height: auto;
  display: inline-block;
  background: white;
  border-radius: 16rpx;
}

.word {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
}

.word>.left {
  float: left;
  margin-left: 24rpx;
}

.word>.right {
  float: right;
  margin-right: 24rpx;
}